{
  "name": "np-select",
  "description": "Nova Poshta city and warehouse selects.",
  "version": "1.0.2",
  "main": "build/np-select.js",
  "scripts": {
    "build": "tsc && vite --config vite.config.js build && npm run build:dts",
    "build:dts": "dts-bundle-generator -o ./build/np-select.d.ts --umd-module-name NpSelect ./src/index.ts",
    "prettier": "prettier . --write",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/derenko/np-select"
  },
  "keywords": [
    "Nova Poshta",
    "novaposhta",
    "select",
    "nova poshta warehouse",
    "nova poshta city",
    "nova poshta api"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/derenko/np-select/issues"
  },
  "homepage": "https://github.com/derenko/np-select",
  "author": "Vasyl Derenko <vasyl.derenko@gmail.com>",
  "devDependencies": {
    "@storybook/addon-essentials": "^7.1.0",
    "@storybook/addon-interactions": "^7.1.0",
    "@storybook/addon-links": "^7.1.0",
    "@storybook/addon-storysource": "^7.1.0",
    "@storybook/blocks": "^7.1.0",
    "@storybook/html": "^7.1.0",
    "@storybook/html-vite": "^7.1.0",
    "@storybook/testing-library": "^0.2.0",
    "dts-bundle-generator": "^8.0.1",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.1.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.6",
    "vite-plugin-css-injected-by-js": "^3.2.1",
    "vite-plugin-static-copy": "^0.17.0"
  }
}
